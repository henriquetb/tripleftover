{
  "name": "tripleftover",
  "version": "0.0.3",
  "description": "Trip Leftover",
  "repository": "https://github.com/henriquetb/tripleftover.git",
  "main": "js/app.js",
  "dependencies": {
    "classnames": "*",
    "flux": "*",
    "keymirror": "*",
    "object-assign": "*",
    "react": "*",
    "jquery": "*"
  },
  "devDependencies": {
    "browserify": "*",
    "envify": "*",
    "jest-cli": "*",
    "reactify": "*",
    "uglify-js": "*",
    "watchify": "*"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "author": "Henrique",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./js"
  }
}
